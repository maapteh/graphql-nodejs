{
    "name": "graphql-nodejs",
    "version": "0.0.1",
    "main": "dist/index.js",
    "license": "UNLICENSED",
    "description": "GraphQL modules",
    "engines": {
        "node": ">=14"
    },
    "repository": "",
    "scripts": {
        "dev": "PROFILE=tst LOG_LEVEL=warn PORT=4000 PROXY='' MOCK=off CACHING=off MEMCACHE=off PLAYGROUND=on TRACING=on PROMETHEUS=off PRODUCTION=off ts-node-dev -r tsconfig-paths/register --respawn --inspect --transpile-only --ignore-watch src/generated/* ./src/index.ts",
        "start": "NODE_ENV=production node dist/src/index.js",
        "-------- BUILDING ------": "------------------------------",
        "compile": "tsc -v && NODE_ENV=production && tsc -p tsconfig.build.json",
        "build": "yarn compile && yarn copy:schemas",
        "copy:schemas": "cd src && find . -name '*.graphql' | xargs tar cvf - | (cd ../dist/src; tar xfp -)",
        "copy:mocks": "cp src/__mocks__/*.json dist/src/__mocks__",
        "schema:linter": "graphql-schema-linter",
        "-------- TESTING ------": "------------------------------------------",
        "test": "NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.config.js",
        "test:watch": "yarn test --watch --notify",
        "test:coverage": "yarn test --collectCoverage",
        "test:watch-coverage": "yarn test --watchAll --notify --collectCoverage",
        "---- auto generating ---": "------------------------------",
        "generate:gql-typings": "graphql-codegen",
        "---- PROFILING ---": "------------------------------",
        "profile:flamegraph": "yarn build && NODE_ENV=production PRODUCTION=on PORT=4000 PROFILE=local LOG_LEVEL=warn SERVER=express MOCK=off CORS=off APOLLO_KEY='service:maapteh-local-4aqgsa:JsrelXohoePxZ8W1AAvDUQ' CACHING=off MEMCACHE=off PLAYGROUND=off TRACING=off PROMETHEUS=off 0x --output-dir ./.flamegraphs -o dist/src/index.js",
        "profile:clinic": "yarn build && NODE_ENV=production PRODUCTION=on PORT=4000 PROFILE=local LOG_LEVEL=warn SERVER=express MOCK=off CORS=off APOLLO_KEY='' CACHING=off MEMCACHE=off PLAYGROUND=off TRACING=off PROMETHEUS=off clinic doctor -- node dist/src/index.js",
        "profile:run-app": "yarn build && NODE_ENV=production PRODUCTION=on PORT=4000 PROFILE=local LOG_LEVEL=warn SERVER=express MOCK=off CORS=off APOLLO_KEY='' CACHING=off MEMCACHE=off PLAYGROUND=off TRACING=off PROMETHEUS=off node dist/src/index.js"
    },
    "_moduleAliases": {
        "@app": "dist/src"
    },
    "dependencies": {
        "@graphql-tools/load-files": "6.2.5",
        "@graphql-tools/merge": "6.2.7",
        "abort-controller": "3.0.0",
        "apollo-cache-control": "0.11.6",
        "apollo-server": "2.19.2",
        "apollo-server-cache-redis": "1.2.3",
        "apollo-server-express": "2.19.2",
        "apollo-server-plugin-response-cache": "0.5.8",
        "atob": "2.1.2",
        "dataloader": "2.0.0",
        "express": "4.17.1",
        "graphql": "15.5.0",
        "graphql-import-node": "^0.0.4",
        "graphql-jit": "0.5.0",
        "graphql-modules": "1.2.1",
        "graphql-query-complexity": "0.7.2",
        "graphql-scalars": "1.7.0",
        "graphql-voyager": "1.0.0-rc.31",
        "https-proxy-agent": "5.0.0",
        "node-fetch": "2.6.1",
        "pino": "6.11.0",
        "prom-client": "13.1.0",
        "reflect-metadata": "0.1.13",
        "tiny-lru": "7.0.6"
    },
    "devDependencies": {
        "0x": "4.10.2",
        "@graphql-codegen/add": "2.0.2",
        "@graphql-codegen/cli": "1.20.1",
        "@graphql-codegen/schema-ast": "1.18.1",
        "@graphql-codegen/time": "2.0.2",
        "@graphql-codegen/typescript": "1.20.2",
        "@graphql-codegen/typescript-resolvers": "1.18.1",
        "@graphql-inspector/cli": "2.3.0",
        "@graphql-inspector/config": "^2.2.0",
        "@types/cookie-parser": "1.4.2",
        "@types/express": "4.17.11",
        "@types/jest": "26.0.20",
        "@types/node-fetch": "2.5.8",
        "@types/pino": "6.3.5",
        "@types/uuid": "8.3.0",
        "blocked": "1.3.0",
        "clinic": "8.0.1",
        "dtsgenerator": "3.7.0",
        "graphql-helix": "^1.2.3",
        "graphql-schema-linter": "0.5.0",
        "inquirer": "7.3.3",
        "jest": "26.6.3",
        "open": "7.4.0",
        "pino-pretty": "4.5.0",
        "pm2": "4.5.4",
        "prettier": "2.2.1",
        "simple-git": "2.34.2",
        "ts-jest": "26.5.0",
        "ts-node": "9.1.1",
        "ts-node-dev": "1.1.1",
        "tsconfig-paths": "3.9.0",
        "typescript": "4.1.3"
    },
    "graphql-inspector": {
        "diff": true,
        "schema": {
            "ref": "master",
            "path": "_schema.graphql"
        }
    }
}
